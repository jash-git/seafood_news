"use strict";$(document).ready(function(){var d,o;$("body").is(".r18")&&(d=$('<div class="r18-prompt"><div class="modal" id="r18-prompt-modal" tabindex="-1" role="dialog"><div class="modal-dialog" role="document"><div class="modal-content"><div class="modal-body text-center"><div class="icon-lock">限</div><div class="warning-text">本文含有成人內容<br>請確定您已年滿18歲</div></div><div class="modal-footer justify-content-center"><a role="button" class="btn btn-lg btn-success" data-dismiss="modal" href="javascript:;">我已滿18歲</a><a role="button" class="btn btn-lg btn-danger" href="/">我未滿18歲</a></div></div></div></div></div>').appendTo("body"),o=d.find("#r18-prompt-modal"),d.on({init:function(d){o.modal({backdrop:"static",keyboard:!1})}}),o.on({"hidden.bs.modal":function(){d.remove()}}),d.trigger("init"))});