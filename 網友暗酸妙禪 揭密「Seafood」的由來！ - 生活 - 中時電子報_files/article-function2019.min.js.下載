"use strict";$(document).ready(function(){var t,i,a=$(".font-size-setting"),e=$(".article-box");a.on({init:function(e){e.stopPropagation(),t=localStorage.getItem("ctBaseFontSize"),i=a.find(".active").attr("data-font-size"),t&&t!==i&&a.find("a").filter('[data-font-size="'+t+'"]').trigger("click")}}),a.on({click:function(){var t=$(this),i=t.attr("data-font-size");t.addClass("active").siblings().removeClass("active"),e.css("font-size",i),localStorage.setItem("ctBaseFontSize",i)}},"a[data-font-size]"),a.trigger("init")});