"use strict";$(document).ready(function(){$(".tab-pane").each(function(){var t=$(this),e=t.find(".tab-menu"),i=t.find(".tab-content"),n=t.find(".owl-carousel"),o=n.length;e.on({setActive:function(t){t.stopPropagation(),t.preventDefault(),$(this).addClass("active").siblings().trigger("removeActive")},removeActive:function(t){t.stopPropagation(),$(this).removeClass("active")}},"li"),e.on({click:function(t){var e=$(this),a=e.parent("li"),r=e.attr("href");t.stopPropagation(),t.preventDefault(),a.trigger("setActive"),o?n.trigger("to.owl.carousel",[a.index()]):i.find(r).trigger("setActive")}},"li > a"),i.on({setActive:function(t){t.stopPropagation(),t.preventDefault(),$(this).addClass("active").siblings().trigger("removeActive")},removeActive:function(t){t.stopPropagation(),$(this).removeClass("active")}},".tab-content-container"),o&&n.on({"changed.owl.carousel":function(t){var i,n,o=$(this);setTimeout(function(){i=o.find(".owl-item.active"),n=i.find(".tab-content-container").attr("data-tab-id"),e.find("#"+n).trigger("setActive")},10)}})})});